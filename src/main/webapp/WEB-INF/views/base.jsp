<body>
	<div class="container">
		<video autoplay muted loop plays-inline class="video">
			<source src="<c:url value= "resources/images/backvideo.mp4" />"
				type="video/mp4" />
		</video>
		<div class="maintext">
			<div class="leftside">
				<div class="first">
					<p>Life's too short to wear boring clothes.</p>
				</div>
				<div class="second">
					<h3>Gets All The Latest Trends Clothes At An Affordable Price</h3>
				</div>
				<button>
					Shop Now <span class="material-symbols-outlined">
						shopping_cart_checkout </span>
				</button>
			</div>
			<div class="rightside">
				<img src="resources/images/bgImage.png" alt="image">
			</div>
		</div>

		<header class="header">
			<div class="logo">
				<a href="/productcrudapp"><span
					class="material-symbols-outlined"> diamond </span></a> <a
					href="/productcrudapp"><h2>Fashion</h2></a>
			</div>

			<nav class="nav">
				<a href="men">Men</a> <a href="women">Women</a> <a href="children">Children</a>
				<a href="offers">Offer</a> <a href="contact">Contact Us</a>
			</nav>

			<div class="icons">
				<a id="search" data-toggle="modal" data-target="#search-modal">
					<span class="material-symbols-outlined"> search </span>
				</a> <a data-toggle="modal" id="favorite" data-target="#wishlist-modal">
					<span class="material-symbols-outlined"> favorite </span>
				</a> <a data-toggle="modal" data-target="#add-post-modal" class="button"
					id="checkout"> <span class="material-symbols-outlined">
						shopping_cart_checkout </span>
				</a> <a id="login" data-toggle="modal" data-target="#login-modal"> <span
					class="material-symbols-outlined"> login </span>
				</a>

				<!-- Cart Modal Starts -->
				<div class="modal fade" id="add-post-modal" tabindex="-1"
					role="dialog" aria-labelledby="exampleModalCenterTitle"
					aria-hidden="true">
					<div class="modal-dialog modal-dialog-centered" role="document">
						<div class="modal-body">
							<div class="checkout">
								<div class="checkbox">
									<img src="<c:url value="resources/images/Boys.jpeg" /> "
										alt="image">
									<h2>avery shirt</h2>
									<p>qty:1</p>
									<span class="material-symbols-outlined"> add </span><span
										class="material-symbols-outlined"> delete </span>
								</div>
								<div class="checkbox">
									<img alt="image"
										src="<c:url value="/resources/images/Boys.jpeg" /> ">
									<h2>Men Cargos</h2>
									<p>qty:2</p>
									<span class="material-symbols-outlined"> add </span><span
										class="material-symbols-outlined"> delete </span>
								</div>

								<div class="checkbox">
									<img src="resources/images/Boys.jpeg" alt="image">
									<h2>mens hoodie</h2>
									<p>qty:1</p>
									<span class="material-symbols-outlined"> add </span><span
										class="material-symbols-outlined"> delete </span>
								</div>
								<button>
									Checkout <span class="material-symbols-outlined">
										shopping_cart </span>
								</button>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- Cart Modal Ends -->

			<!-- Wishlist Modal Starts -->
			<div class="modal fade" id="wishlist-modal" tabindex="-1"
				role="dialog" aria-labelledby="exampleModalCenterTitle"
				aria-hidden="true">
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-body">
						<div class="favorites" id="favorites1">
							<h1>You</h1>
							<a href="#"><span class="material-symbols-outlined">
									favorite </span></a>
						</div>
					</div>
				</div>
			</div>
			<!-- Wishlist Modal Ends -->

			<!-- Search Modal Starts -->
			<div class="modal fade" id="search-modal" tabindex="-1" role="dialog"
				aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-body">
						<div class="searchbar">
							<h1>Search......</h1>
							<a href="#"><span class="material-symbols-outlined">
									search </span></a>
						</div>
					</div>
				</div>
			</div>
			<!-- Search Modal Ends -->

			<!-- Login Modal Starts -->
			<div class="modal fade" id="login-modal" tabindex="-1" role="dialog"
				aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-content">
						<div class="modal-body">
							<form action="/productcrudapp/" method="post">
								<div class="login">
									<input type="text" id="Username" placeholder="Username"
										name="name" autocomplete="true"> <input
										type="password" id="password" placeholder="Password"
										autocomplete="true" name="password"> <a
										href="register">
										<p>Or new user?</p> Register Here <span
										class="material-symbols-outlined"> login </span>
									</a>
									<button type="submit">signin</button>
								</div>

							</form>

														<div class="card">
									<div
										class="card-header primary-background text-white text-center">
										<span class="fa fa-user-plus fa-3x"></span> <br>
										<p class="text-primary">Login here</p>
									</div>
									<div>
										<div class="card-body">
											<form action="/productcrudapp/" method="post">
												<div class="form-group">
													<label for="exampleInputEmail1">Email address</label> <input
														name="email" required type="email"
														class="form-control text-primary" id="exampleInputEmail1"
														aria-describedby="emailHelp" placeholder="Enter email">

												</div>
												<div class="form-group">
													<label for="exampleInputPassword1">Password</label> <input
														name="password" required type="password"
														class="form-control text-primary"
														id="exampleInputPassword1" placeholder="Password">
												</div>

												<div class="conatiner text-center">
													<a href="profile.jsp" type="submit" class="btn btn-primary">Submit</a>
													<button type="submit" class="btn btn-primary">Submit</button>
												</div>
												<br>
												<p class="text-primary">Or new User?</p>
												<button type="submit" class="btn btn-primary">Register</button>
											</form>
									</div>
									</div>
									</form>
								</div>

						</div>
					</div>
				</div>
				<!-- Login Modal Ends -->
			</div>
